{
  "name": "express-api-typescript-boilerplate",
  "version": "1.0.0",
  "description": "An express API boilerplace with type support",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn ./src/index.ts",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "test:watch": "jest -i --watchAll",
    "test:coverage": "jest -i --coverage",
    "lint": "npx eslint **/*.ts",
    "lint:fix": "npx prettier-eslint **/*.ts --write",
    "prepare": "husky install",
    "docker:dev": "docker-compose -f docker-compose.yaml -f docker-compose.dev.yaml up"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frasnym/Express-API-TypeScript-Boilerplate.git"
  },
  "keywords": [],
  "author": "Nyoman Frastyawan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frasnym/Express-API-TypeScript-Boilerplate/issues"
  },
  "homepage": "https://github.com/frasnym/Express-API-TypeScript-Boilerplate#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.37",
    "@types/passport-jwt": "^3.0.5",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "node-mocks-http": "^1.10.1",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.5.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2",
    "swagger-jsdoc": "^6.0.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  }
}
